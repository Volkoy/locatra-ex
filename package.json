		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"wrangler": "^4.30.0"
	},
	"dependencies": {
		"@google/genai": "^1.30.0",
		"@inlang/paraglide-js": "^2.0.0",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.55.0",
